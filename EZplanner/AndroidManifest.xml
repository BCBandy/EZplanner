<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ezplanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Device_fragment"
            android:label="@string/title_activity_device_fragment" >
        </activity>
        <activity
            android:name=".ListItems"
            android:label="@string/title_activity_list_items" >
        </activity>
        <activity
            android:name=".AddItem"
            android:label="@string/title_activity_date_picker" >
        </activity>
        <activity
            android:name=".Details"
            android:label="@string/title_activity_details" >
        </activity>
        <activity
            android:name=".FragmentStart"
            android:label="@string/title_activity_fragment_start" >
        </activity>
        <activity
            android:name=".DayItemActivity"
            android:label="@string/title_activity_day_item" >
        </activity>
        <activity
            android:name=".SingleItemView"
            android:label="@string/title_activity_single_item_view" >
        </activity>
        <activity
            android:name=".TextEditor"
            android:label="@string/title_activity_text_editor" >
        </activity>
        <activity
            android:name=".OldList"
            android:label="@string/title_activity_empty_list" >
        </activity>
    </application>

</manifest>
